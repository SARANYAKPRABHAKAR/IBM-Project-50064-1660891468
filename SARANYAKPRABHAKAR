        "import datetime\n",
        "\n",
        "import pandas as pd\n",
        " \n",
        "# initializing date\n",
        "\n",
        "test_date = datetime.datetime.strptime(\"01-7-2022\", \"%d-%m-%Y\")\n",
        " \n",
        "# initializing K \n",
        "\n",
        "K = 5\n",
        " \n",
        "\n",
        "date_generated = pd.date_range(test_date, periods=K)\n",
        "\n",
        "print(date_generated.strftime(\"%d-%m-%Y\"))"
      ],
